{
  "name": "zy-video",
  "version": "1.0.0",
  "description": "中宜教育视频系统",
  "main": "./app/main.js",
  "scripts": {
    "dev": "webpack --mode development",
    "serve": "electron .",
    "start": "npm-run-all --parallel dev serve",
    "build": "webpack --mode production --config webpack.pro.config.js",
    "packager:mac": "electron-packager ./app zy-video --darwin --out ./release --arch=x64 --overwrite --version=1.0.0 --electron-version=1.8.4 --ignore=src --asar",
    "packager:win": "electron-packager ./app zy-video --win --out ./release --arch=x64 --overwrite --version=1.0.0 --electron-version=1.8.4 --ignore=src --asar",
    "release:mac": "npm-run-all build packager:mac",
    "release:win": "npm-run-all build packager:win"
  },
  "author": "sjy",
  "devDependencies": {
    "asar": "^0.14.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-packager": "^12.0.1",
    "electron-reload": "^1.2.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-persist-electron-storage": "^1.1.2",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0"
  }
}
